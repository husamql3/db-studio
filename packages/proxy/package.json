{
  "name": "proxy",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "wrangler build",
    "start": "bun run build && wrangler dev --remote",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "check": "biome check --write --unsafe --verbose",
    "secrets:add": "wrangler secret bulk .env",
    "secrets:list": "wrangler secret list",
    "secrets:clear": "wrangler secret delete --all"
  },
  "dependencies": {
    "@tanstack/ai": "^0.2.2",
    "@tanstack/ai-anthropic": "^0.2.0",
    "@tanstack/ai-gemini": "^0.3.2",
    "@tanstack/ai-openai": "^0.3.0",
    "@upstash/redis": "^1.36.1",
    "hono": "^4.11.3",
    "hono-rate-limiter": "^0.5.3",
    "shared": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "wrangler": "^4.4.0"
  }
}