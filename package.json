{
  "name": "db-studio",
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "www"
  ],
  "scripts": {
    "dev": "bun run --cwd packages/core dev",
    "dev:www": "bun run --cwd www dev",
    "build": "bun run --cwd packages/core build",
    "build:www": "bun run --cwd www build",
    "check": "bun run --cwd packages/core check && bun run --cwd www check"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.x",
    "bun": ">=1.2.19"
  },
  "packageManager": "bun@1.2.19"
}