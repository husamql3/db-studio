{
  "name": "db-studio",
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "www"
  ],
  "scripts": {
    "dev": "bun run --cwd packages/core dev",
    "dev:www": "bun run --cwd www dev",
    "dev:server": "bun run --cwd packages/core dev:server",
    "build:core": "bun run --cwd packages/core build && bun run --cwd packages/core build:server",
    "build:www": "bun run --cwd www build",
    "build:server": "bun run --cwd packages/core build:server",
    "check": "bun run --cwd packages/core check && bun run --cwd www check"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.x",
    "bun": ">=1.2.19"
  },
  "packageManager": "bun@1.2.19",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.13"
  }
}